javascript:(function(){function footer(){let e=document.createElement("div");var t=0,o=0,n=0,i=0;e.onmousedown=(l=window.event)=>{l.preventDefault(),n=l.clientX,i=l.clientY,document.onmouseup=()=>{document.onmouseup=null,document.onmousemove=null},document.onmousemove=l=>{(l=l||window.event).preventDefault(),t=n-l.clientX,o=i-l.clientY,n=l.clientX,i=l.clientY;let d=e.offsetTop-o>0?e.offsetTop-o:0,u=e.offsetLeft-t>0?e.offsetLeft-t:0;e.style.top=d+"px",e.style.left=u+"px"}}}(async()=>{let e=document.createElement("iframe");if(document.body.append(e),window.alert=e.contentWindow.alert.bind(window),window.prompt=e.contentWindow.prompt.bind(window),window.confirm=e.contentWindow.confirm.bind(window),e.remove(),"/play/fishing"!=window.location.pathname)alert("You must be in a fishing frenzy game!");else{let e=Number(parseFloat(prompt("What do you want to set your lure to? (1 - 5)")))-1;Object.values(document.querySelector("#app > div > div"))[1].children[1]._owner.stateNode.setState({lure:e<0?0:e>4?4:e}),alert("Lure set!")}})(),footer();}());
